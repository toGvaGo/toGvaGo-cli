#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code/learning/实战项目/脚手架/mortal/packages/mortal-cli/bin/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/packages/mortal-cli/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/packages/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/node_modules:/mnt/d/code/learning/实战项目/脚手架/node_modules:/mnt/d/code/learning/实战项目/node_modules:/mnt/d/code/learning/node_modules:/mnt/d/code/node_modules:/mnt/d/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code/learning/实战项目/脚手架/mortal/packages/mortal-cli/bin/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/packages/mortal-cli/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/packages/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/node_modules:/mnt/d/code/learning/实战项目/脚手架/node_modules:/mnt/d/code/learning/实战项目/node_modules:/mnt/d/code/learning/node_modules:/mnt/d/code/node_modules:/mnt/d/node_modules:/mnt/d/code/learning/实战项目/脚手架/mortal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mortal-cli/bin/index.js" "$@"
else
  exec node  "$basedir/../mortal-cli/bin/index.js" "$@"
fi
